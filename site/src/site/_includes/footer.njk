<footer class='f jcb fw rel aist alt js-alt  bcw footer' data-component='footer'>
  <div class='x c50 footer__main'>
    <div class='x p15 bb footer__block footer__logo'>
      <div class='footer__inner footer__logo mxa cg'>
        {% include 'logo.njk' %}
      </div>
    </div>
    <div class='footer__block-wrapper'>
      <div class='al p15 bb footer__block'>
        <div class='footer__inner footer__description mxa'>
          <span class='caps'>WN V1.0 {{ site.buildTime | dateDisplay('LLL d, yyyy - HH:mm') }}</span>
          <p class='s28 bold'>
            This website it BETA.<a href='/versioning' class='cg'>Version 1.0</a>
          </p>
          <a href='/versioning' class='button caps'>Versioning &amp; How to Support</a>
        </div>
      </div>
      <div class='p15 al footer__block'>
        <div class='footer__inner mxa'>
          <ul class='footer__links m0 p0 f fw jcb'>
            <li><a class='s28 mb05' href='/listings'>Offerings</a></li>
            <li><a class='s28 mb05' href='mailto:reachout@enoughstructures.com'>Contact</a></li>
            <li><a class='s28 mb05' href='http://instagram.com/enoughstructures' rel='noopener' target='_blank'>Instagram</a></li>
            <li><a class='s28 mb05' href='/mission'>Mission</a></li>
            <!-- <li><a class='s28 mb05' href='/contribution-guidelines'>Guidelines</a></li> -->
            <li><a class='s28 mb05' href='/submit'>Submit</a></li>
          </ul>
        </div>
      </div>
    </div>
  </div>
  <div class='ac c50 p15 newsletter x rel' data-component="form">
    <div class='footer__credits f jcc aic js-credits bcg abs x y left top z1'>
      <div class='al p15 footer__credits-inner mxa'>
        <picture>
          <source type='image/webp' srcset='/images/straw-homies.webp' />
          <img class='x my1' data-component='image' data-src='/images/straw-homies.png' alt='Homies' />
        </picture>
        <h3 class='s28'>Enough Structures is run as part of <a href='https://infiniteplaces.org' class='cw' rel='noopener' target='_blank'>Infinite Places</a></h3>
      </div>
    </div>
    <div class='footer__inner py1 mxa'>
      <picture>
        <source type='image/webp' srcset='/images/cooking.webp' />
        <img class='x footer__illy mxa' data-component='image' data-src='/images/cooking.png' alt='camp cooking' />
      </div>
      <span class='s28 mt1 block pt05'>Subscribe to the <span class='cg'>Enough Structures</span> newsletter</span>
      <p class=''>Not junk mail; Just profiles on suppliers, materials, innovators and more.</p>
      <form class="newsletter__form js-subscribe rel">
        <label for='email'><span style='display: none;'>Email</span>
          <input aria-label='newsletter email' required class=" x py05 serif s22 container--s" type="email" name="email" placeholder='Email' />
        </label>
        <button type="submit" class="caps bcp abs bold bottom right">
          <span>Submit</span>
        </button>
      </form>
      <div class='s28 hidden js-spaghetti' style='margin-top: 1.5rem;'>
        We'll keep it short and sweet!
      </div>
    </div>
  </div>
</footer>
