<div class='x category__filters' data-component='filters'>
  <div class='category__filters-inner x p1 al bcw '>
    <span class='s18'>Filter</span>
    <div class='category__filters-group js-check-filters al mt1'>
      <div>
        <input type="checkbox" id="biodegredable" name="biodegredable" value='biodegredable'>
        <label for="biodegredable">Biodegredable</label>
      </div>
      <div>
        <input type="checkbox" id="compostable" name="compostable" value='compostable'>
        <label for="compostable">Compostable</label>
      </div>
      <div>
        <input type="checkbox" id="home-compostable" name="home-compostable" value='home-compostable'>
        <label for="home-compostable">Home Compostable</label>
      </div>
      <div>
        <input type="checkbox" id="renewed-material" name="renewed-material" value='renewed-material'>
        <label for="renewed-material">Renewed Material</label>
      </div>
      <div>
        <input type="checkbox" id="bio-based" name="bio-based" value='bio-based'>
        <label for="bio-based">Bio Based</label>
      </div>
      <div>
        <input type="checkbox" id="dissolvable" name="dissolvable" value='dissolable'>
        <label for="dissolvable">Dissolvable</label>
      </div>
      <div>
        <input type="checkbox" id="recyclable" name="recyclable" value='recyclable'>
        <label for="recyclable">Recyclable</label>
      </div>
    </div>
  </div>
  <div class='category__filters-inner x p1 al bcw '>
    <span class='s18'>Quantities</span>
    <div class='category__filters-group js-quantites al mt1'>
      <div>
        <input type="radio" name='quantities' id="low" value="low">
        <label for="low">Low Minimums</label>
      </div>
      <div>
        <input type="radio" name='quantities' id="mixed" value="mixed">
        <label for="mixed">Mixed Minimums</label>
      </div>
      <div>
        <input type="radio" name='quantities' id="high" value="high">
        <label for="high">High Minimums</label>
      </div>
      <div>
        <input checked type="radio" name='quantities' id="show-all" value="show-all">
        <label for="show-all">Show all</label>
      </div>
    </div>
  </div>
  <div class='category__filters-inner x p1 al bcw '>
    <span class='s18'>Origin</span>
    <div class='category__filters-group al mt1'>
      <div>
        <select id="multiple" multiple>
          <option value="china">China</option>
          <option value="new-zealand">New Zealand</option>
          <option value="vietnam">Vietnam</option>
          <option value="usa">United States of America</option>
        </select>
      </div>
    </div>
  </div>

</div>